--ui

--affiche ui
function draw_ui()
	camera()
	--gerer la transparence palt
	palt(0,false)
	palt(12,true)
	
	spr(6,2,2)
	palt()
	
	print_outline("X"..p.keys,10,4)
end

--fonction pour outline l'ui
function print_outline(text,x,y)
	print(text,x-1,y,0)
	print(text,x+1,y,0)
	print(text,x,y-1,0)
	print(text,x,y+1,0)
	print(text,x,y,7)
end
